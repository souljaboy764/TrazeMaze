<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trazemaze2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.trazemaze2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.trazemaze2.PracticeActivity"
            android:label="@string/title_activity_practice" >
        </activity>
        <activity
            android:name="com.example.trazemaze2.EnglishActivity"
            android:label="@string/title_activity_english" >
        </activity>
        <activity
            android:name="com.example.trazemaze2.HindiActivity"
            android:label="@string/title_activity_hindi" >
        </activity>
        <activity
            android:name="com.example.trazemaze2.ArabicActivity"
            android:label="@string/title_activity_arabic" >
        </activity>
    </application>

</manifest>
